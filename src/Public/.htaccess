RewriteEngine On
RewriteRule ^([A-z0-9_/-]*)$ index.php?p=$1

ErrorDocument 403 /index.php?p=Error/not_authorized
ErrorDocument 404 /index.php?p=Error/not_found


<FilesMatch "\.(css|js)$">
  Header set Cache-Control "max-age=604800, public"
</FilesMatch>

<FilesMatch "\.(img|svg)$">
  Header set Cache-Control "max-age=604800, public"
</FilesMatch>
